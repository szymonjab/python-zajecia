import pytest  # <- import pytest module
import time  # <- import time module
from selenium import webdriver
from selenium.webdriver.common.by import By
from selenium.webdriver.common.keys import Keys


driver = webdriver.Chrome()

driver.get("https://www.selenium.dev/selenium/web/web-form.html")
title = driver.title
print(title)

text_input = driver.find_element(by=By.NAME, value="my-text")
submit_button = driver.find_element(by=By.CSS_SELECTOR, value="button")

time.sleep(1)
text_input.send_keys("Selenium")
time.sleep(2)
submit_button.click()


time.sleep(10)  # <- wait for 5 seconds
driver.quit()


######


import pytest  # <- import pytest module
import time  # <- import time module
from selenium import webdriver
from selenium.webdriver.common.by import By
from selenium.webdriver.common.keys import Keys




driver = webdriver.Chrome()
driver.maximize_window()
driver.implicitly_wait(3)
driver.get("https://demoqa.com/automation-practice-form")

first_name_input = driver.find_element(By.ID, "firstName")
first_name_input.send_keys("Artur")

last_name_input = driver.find_element(By.ID, "lastName")
last_name_input.send_keys("Babinski")

gender_male = driver.find_element(By.CSS_SELECTOR, "label[for='gender-radio-1']")
gender_male.click()

phone_number_input = driver.find_element(By.ID, "userNumber")
phone_number_input.send_keys("1234567890")

submit_button = driver.find_element(By.ID, "submit")
time.sleep(3)
submit_button.click()

time.sleep(3)
driver.quit()
